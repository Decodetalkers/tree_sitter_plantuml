{
  "name": "plantuml",
  "rules": {
    "plantuml": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "baseuml"
        },
        {
          "type": "SYMBOL",
          "name": "mindmap"
        },
        {
          "type": "SYMBOL",
          "name": "yamluml"
        },
        {
          "type": "SYMBOL",
          "name": "jsonuml"
        }
      ]
    },
    "typedef": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_abstract"
        },
        {
          "type": "SYMBOL",
          "name": "_interface"
        }
      ]
    },
    "_abstract": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "abstract"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "class"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "_interface": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "interface"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "baseuml": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@startuml"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "typedef"
          }
        },
        {
          "type": "STRING",
          "value": "@enduml"
        }
      ]
    },
    "mindmap": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@startmindmap"
        },
        {
          "type": "STRING",
          "value": "@endmindmap"
        }
      ]
    },
    "yamluml": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@startyaml"
        },
        {
          "type": "STRING",
          "value": "@endyaml"
        }
      ]
    },
    "jsonuml": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@startjson"
        },
        {
          "type": "STRING",
          "value": "@endjson"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z]+"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

